<p id="notice"><%= notice %></p>

<h1>Listing Operating Systems</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @operating_systems.each do |operating_system| %>
      <tr>
        <td><%= operating_system.name %></td>
        <td><%= operating_system.price %></td>
        
        <td><%= link_to 'Show', operating_system %></td>
        <td><%= link_to 'Edit', edit_operating_system_path(operating_system) %></td>
        <td><%= link_to 'Destroy', operating_system, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Operating system', new_operating_system_path %>
