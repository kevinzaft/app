<p id="notice"><%= notice %></p>

<h1>Listing Home Offices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @home_offices.each do |home_office| %>
      <tr>
        <td><%= home_office.name %></td>
        <td><%= home_office.price %></td>
        <td><%= link_to 'Show', home_office %></td>
        <td><%= link_to 'Edit', edit_home_office_path(home_office) %></td>
        <td><%= link_to 'Destroy', home_office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home office', new_home_office_path %>
