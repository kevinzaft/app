<p id="notice"><%= notice %></p>

<h1>Listing Android Tvs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @android_tvs.each do |android_tv| %>
      <tr>
        <td><%= android_tv.name %></td>
        <td><%= android_tv.price %></td>
        <td><%= link_to 'Show', android_tv %></td>
        <td><%= link_to 'Edit', edit_android_tv_path(android_tv) %></td>
        <td><%= link_to 'Destroy', android_tv, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Android tv', new_android_tv_path %>
