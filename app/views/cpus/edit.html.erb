<h1>Edit</h1>

<%= form_for(@cpu) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :id %><br />
    <%= f.text_field :code, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :price %><br />
    <%= f.text_field :price, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :cpu_type %><br />
    <%= f.text_field :cputype, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :socket_type %><br />
    <%= f.text_field :sockettype, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :brand %><br />
    <%= f.text_field :brand, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label "Manufacturer Part Number" %><br />
    <%= f.text_field :mpn, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :cpu_model %><br />
    <%= f.text_field :cpumodel, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :series %><br />
    <%= f.text_field :series, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :cores %><br />
    <%= f.text_field :cores, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :threads %><br />
    <%= f.text_field :threads, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :clock %><br />
    <%= f.text_field :clock, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :cache %><br />
    <%= f.text_field :cache, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :power %><br />
    <%= f.text_field :power, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :manufacturing %><br />
    <%= f.text_field :manufacturing, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :memorytypes %><br />
    <%= f.text_field :memorytypes, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :memorychannels %><br />
    <%= f.text_field :memorychannels, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :graphics %><br />
    <%= f.text_field :graphics, :size => 150 %>
  </div>

  <div class="field">
    <%= f.label :other %><br />
    <%= f.text_area :other, cols: "150", rows: "10" %>
  </div>  

  <%= f.submit %>
<% end %>
