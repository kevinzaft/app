<p id="notice"><%= notice %></p>

<h1>Listing Car Accessories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_accessories.each do |car_accessory| %>
      <tr>
        <td><%= car_accessory.name %></td>
        <td><%= car_accessory.price %></td>
        <td><%= link_to 'Show', car_accessory %></td>
        <td><%= link_to 'Edit', edit_car_accessory_path(car_accessory) %></td>
        <td><%= link_to 'Destroy', car_accessory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car accessory', new_car_accessory_path %>
