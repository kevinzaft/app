<p id="notice"><%= notice %></p>

<h1>Listing Karaoke Speakers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @karaoke_speakers.each do |karaoke_speaker| %>
      <tr>
        <td><%= karaoke_speaker.name %></td>
        <td><%= karaoke_speaker.price %></td>
        <td><%= link_to 'Show', karaoke_speaker %></td>
        <td><%= link_to 'Edit', edit_karaoke_speaker_path(karaoke_speaker) %></td>
        <td><%= link_to 'Destroy', karaoke_speaker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Karaoke speaker', new_karaoke_speaker_path %>
