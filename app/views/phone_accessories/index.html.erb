<p id="notice"><%= notice %></p>

<h1>Listing Phone Accessories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_accessories.each do |phone_accessory| %>
      <tr>
        <td><%= phone_accessory.name %></td>
        <td><%= phone_accessory.price %></td>
        <td><%= link_to 'Show', phone_accessory %></td>
        <td><%= link_to 'Edit', edit_phone_accessory_path(phone_accessory) %></td>
        <td><%= link_to 'Destroy', phone_accessory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone accessory', new_phone_accessory_path %>
